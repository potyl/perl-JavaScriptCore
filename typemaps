INPUT

T_JS_GLOBAL_CONTEXT
     if (!gperl_sv_is_ref ($arg)) {
          $var = NULL;
     }
     else {
          $var = INT2PTR($type, SvIV(SvRV($arg)));
     }



OUTPUT

